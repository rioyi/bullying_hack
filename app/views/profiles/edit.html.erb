<h1>Nuevooo Perfil</h1>



<%= @user = current_user.id %>

<%= form_with model: @profile, local: true do |f| %>
	<%= f.label :name,'Nombre' %>
  <%= f.text_field :name %>
  <%= f.label :last_name, 'Apellido' %>
  <%= f.text_field :last_name %>
  <%= f.hidden_field(:user_id, value: @user) %>

  <%= f.submit %>



<%end%>

<%= link_to 'Back', profiles_path%>